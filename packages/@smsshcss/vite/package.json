{
  "name": "@smsshcss/vite",
  "version": "2.0.0",
  "description": "Vite plugin for smsshcss to generate CSS only for classes used in HTML",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "smsshcss": "^1.0.0"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.8",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^8.56.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.0"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "css",
    "utility",
    "smsshcss"
  ]
}
